{
    "builds": [{
        "src":"inventoryproject/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9", "buildCommand": "pip install -r requirements.txt"}

    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "inventoryproject/wsgi.py"
        }
    ]
}